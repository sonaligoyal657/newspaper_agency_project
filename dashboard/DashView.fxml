<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane minHeight="600.0" minWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="dashboard.DashViewController">
   <children>
      <Label layoutX="230.0" layoutY="40.0" text="Dasboard" underline="true">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Button fx:id="btnpaper" layoutX="92.0" layoutY="215.0" mnemonicParsing="false" onAction="#doPaper" text="Newspapers" />
      <Button fx:id="btnhawkmanager" layoutX="327.0" layoutY="325.0" mnemonicParsing="false" onAction="#doHawkManager" text="Hawker Manager" />
      <Button fx:id="btncustomer" layoutX="204.0" layoutY="325.0" mnemonicParsing="false" onAction="#doCustomer" prefHeight="25.0" prefWidth="111.0" text="View Customers" />
      <Button fx:id="btngenerate" layoutX="204.0" layoutY="215.0" mnemonicParsing="false" onAction="#doGenerate" text="Bill Generator" />
      <Button fx:id="btnhistory" layoutX="421.0" layoutY="215.0" mnemonicParsing="false" onAction="#doHistory" text="Bill History" />
      <Button fx:id="btncollect" layoutX="314.0" layoutY="215.0" mnemonicParsing="false" onAction="#doCollect" text="Bill Collector" />
      <Button fx:id="btncustmanager" layoutX="74.0" layoutY="325.0" mnemonicParsing="false" onAction="#doCustManager" text="Customer Manager" />
      <Button fx:id="btnhawkers" layoutX="449.0" layoutY="325.0" mnemonicParsing="false" onAction="#doHawkers" text="View Hawkers" />
   </children>
</AnchorPane>
